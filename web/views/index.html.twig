{% extends "layout.html" %}
{% block content %}
<body>
    <div class="container">
        <br>
        <div class="text-center"><img src="images/dadafund_logo.png" class="img-responsive" alt="DaDaFund" width=150 style="margin:0 auto;"></div>
        <h3 class="text-center">Login</h3>
        <form>
            <div class="form-group">
                <label for="email">Email</label>
                <input type="email" class="form-control" name="email" id="email" placeholder="Enter a valid email ID">
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                <input type="password" class="form-control" name="password" id="password" placeholder="Enter a valid password">
            </div>
            <button type="button" class="btn btn-primary">Login</button>
        </form>
    </div>
    <div class="container-fluid">
        <div id="myModal" class="modal fade" role="dialog"></div>
    </div>
    <br>
    <br>
    <br>
    <div id="footer" onclick="window.location='https://twitter.com/thedustmakers';"></div>
</body>
{% endblock %}